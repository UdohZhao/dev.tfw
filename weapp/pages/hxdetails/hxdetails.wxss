/* hxdetails.wxss */
/* 在使用的Wxss中引入WxParse.css,可以在app.wxss */
@import "../../dist/wxParse/wxParse.wxss";
page{
  background: #fff;
}
.house_swiper image{
  width: 100%;
  height: 100%;
}
.house_name{
  background-color: #fff;
  padding: 10px 15px;
  box-sizing: border-box;
}
.introduction{
  display: inline-block;
  margin-right: 5px;
  padding: 3px 5px;
  border: 1px solid #B5495B;
  border-radius: 4px;
  margin-bottom: 3px;
}
.content_details{
  line-height: 22px;
}
.belong{
  font-size: 16px;
}
.belong text{
  color: #00ce3c;
}
.weui-cells::after{
  border-bottom: none;
}
.analysis-box{
  margin: 0 30rpx;
}
.mu-price{
  font-size: 32rpx;  
  color: #B5495B;
}
.sell_type{
  float: right;
  color: #B5495B;
}